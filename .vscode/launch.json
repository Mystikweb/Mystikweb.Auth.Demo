{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch App Host",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Mystikweb.Auth.Demo.AppHost/bin/Debug/net9.0/Mystikweb.Auth.Demo.AppHost.dll",
            "launchSettingsProfile": "https",
            "launchSettingsFilePath": "${workspaceFolder}/src/Mystikweb.Auth.Demo.AppHost/Properties/launchSettings.json",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "Login to the dashboard at (https?://\\S+)",
                "uriFormat": "%s"
            }
        }
    ]
}
